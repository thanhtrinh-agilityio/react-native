{
  "name": "chatrak-gpt",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "storybook-generate": "sb-rn-get-stories",
    "prepare": "husky install chatrak-gpt/.husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@gorhom/bottom-sheet": "5.1.4",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/drawer": "^7.3.12",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@rneui/base": "^4.0.0-rc.8",
    "@rneui/themed": "^4.0.0-rc.8",
    "@tanstack/react-query": "^5.76.1",
    "expo": "53.0.9",
    "expo-auth-session": "~6.1.5",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.4",
    "expo-dev-client": "~5.1.8",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.1.7",
    "expo-image-picker": "~16.1.4",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.4",
    "expo-modules-core": "~2.3.12",
    "expo-random": "^14.0.1",
    "expo-router": "~5.0.6",
    "expo-splash-screen": "~0.30.8",
    "expo-sqlite": "~15.2.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.7",
    "expo-web-browser": "~14.1.6",
    "firebase": "^11.7.3",
    "franc-min": "^6.2.0",
    "langs": "^2.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.56.3",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-gifted-chat": "^2.8.1",
    "react-native-keyboard-controller": "^1.17.2",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-toast-message": "^2.3.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ],
    "moduleNameMapper": {
      "@/assets/(.*)$": "<rootDir>/assets/$1",
      "^@/(.*)$": "<rootDir>/$1",
      "^expo/fetch$": "<rootDir>/__mocks__/expo/fetch.ts"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|tamagui|@tamagui|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/components/**/*.{ts,tsx}",
      "**/hooks/**/*.{ts,tsx}",
      "**/services/**/*.{ts,tsx}",
      "!**/*.stories.{ts,tsx}"
    ]
  },
  "lint-staged": {
    "./*.{js,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.3.0",
    "@react-native-community/slider": "4.5.6",
    "@storybook/addon-ondevice-actions": "8.6.2",
    "@storybook/addon-ondevice-controls": "8.6.2",
    "@storybook/react-native": "8.6.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "babel-loader": "8.4.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "react-native-svg": "15.11.2",
    "react-test-renderer": "19.0.0",
    "typescript": "^5.8.3"
  },
  "private": true
}
